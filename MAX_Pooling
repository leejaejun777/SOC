module row_max4 (
    input  wire signed [31:0] in0,
    input  wire signed [31:0] in1,
    input  wire signed [31:0] in2,
    input  wire signed [31:0] in3,
    output wire signed [31:0] max_out
);
    wire signed [31:0] max01;
    wire signed [31:0] max23;

    assign max01 = (in0 >= in1) ? in0 : in1;
    assign max23 = (in2 >= in3) ? in2 : in3;
    assign max_out = (max01 >= max23) ? max01 : max23;
endmodule
